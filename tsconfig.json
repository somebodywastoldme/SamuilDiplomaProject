{
	"compilerOptions": {
		"target": "es6",
		"module": "esnext",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"allowJs": false,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"lib": [
			"esnext",
			"es5",
			"es6",
			"es7",
			"es2017",
			"dom",
			"esnext.asynciterable"
		],
		"baseUrl": "./",
		"paths": {
			"reactApp/*": [
				"./App/*"
			]
		},
		"plugins": [
			{
				"name": "typescript-plugin-css-modules",
				"options": {
					"customMatcher": "\\.scss$"
				}
			}
		]
	}
}